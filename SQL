SELECT * FIRST_NAME, LAST_NAME
FROM WORKER
WHERE SALARY >=50000 AND SALARY <=100000;

SELECT COUNT(*), DEPARTMENT
FROM WORKER
GROUP BY DEPARTMENT
ORDER BY COUNT(*)
DESC;

SELECT *
FROM WORKER
INNER JOIN BONUS ON WORKER.ID = BONUS.WORKER_ID;

SELECT SALARY
FROM WORKER
ORDER BY SALARY
OFFSET 4 ROWS
FETCH NEXT 1 ROWS ONLY;
